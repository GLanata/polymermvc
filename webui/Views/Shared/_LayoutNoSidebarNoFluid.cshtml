  </head>

  <body>

    @Html.Partial("_NavBar")

    <div class="container"><br/>
      <div class="row">
        
        <div class="span12">
          @RenderBody()  
        </div>

      </div>
    </div>
    
    <div class="container-fluid footerText">
      <p>&copy;2013 Digital Solutions Computer Software Ltd</p>
    </div>
    
    @Html.Partial("_MainFootScripts")
